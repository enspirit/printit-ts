{
  "name": "@enspirit/printit",
  "version": "2.0.3",
  "description": "Printit",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:watch": "tsc -w",
    "test": "mocha -r ts-node/register tests/helpers.ts 'tests/**/*.spec.ts'",
    "test:watch": "mocha -r ts-node/register tests/helpers.ts 'tests/**/*.spec.ts' --watch --watch-files src/**/*.ts,tests/**/*.ts",
    "lint": "eslint src tests --ext ts",
    "preversion": "npm run build",
    "prepublishOnly": "npm run build",
    "start": "node dist/src/index.js",
    "dev": "tsc-watch --onSuccess 'node ./dist/src/index.js'"
  },
  "keywords": [],
  "author": "@enspirit srl",
  "license": "MIT",
  "files": [
    "dist/src",
    "dist/package.json"
  ],
  "exports": {
    ".": "./dist/src/index.js"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "js-yaml": "^4.1.0",
    "winston": "^3.8.2",
    "sanitize-html": "^2.7.3",
    "html-to-docx": "^1.6.4"
  },
  "devDependencies": {
    "@enspirit/eslint-config-node": "^0.1.5",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.1",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@types/supertest": "^2.0.12",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.20.0",
    "mocha": "^10.0.0",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "ts-watch": "^1.0.8",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  }
}
